# this shows the world what version of apache we're running, so turn it off
ServerSignature Off
ServerTokens Prod

RewriteLog      "/var/log/apache2/rewrite.log"
RewriteLogLevel 3

# run apache as apache, not root, or whatever else it runs as
# TODO - need to create the user for this to work properly
#User apache
#Group apache

# Proxy Passes
<IfModule mod_proxy.c>
        ProxyPass        /docker http://localhost:50000
        ProxyPassReverse /docker http://localhost:50000
</IfModule>
